ins)\n","\n","    # need at least 2 domain groups and 2 Topic classes\n","    if fair[\"Topic\"].nunique() < 2:\n","        print(\"Fairness: only one Topic class after filtering, skipping.\")\n","    elif fair[\"domain_group\"].nunique() < 2:\n","        print(\"Fairness: only one domain_group, skipping.\")\n","    else:\n","        X = fair[\"original_text\"].astype(str)\n","        y = fair[\"Topic\"].astype(str)\n","\n","        X_train, X_test, y_train, y_test = train_test_split(\n","            X, y, test_size=0.2, random_state=SEED, stratify=y\n","        )\n","\n","        pipe = Pipeline([\n","            (\"tfidf\", TfidfVectorizer(max_features=15000, ngram_range=(1,2))),\n","            (\"lr\", LogisticRegression(max_iter=400))\n","        ])\n","        pipe.fit(X_train, y_train)\n","        pred = pipe.predict(X_test)\n","        base = f1_score(y_test, pred, average=\"macro\")\n","        print(f\"[Overall Topic] macroF1={base:.3f}\")\n","\n","        # per-domain metrics on the test slice\n","        groups = fair.loc[X_test.index, \"domain_group\"]\n","        for g in groups.unique():\n","            mask = (groups == g)\n","            if mask.sum() >= 15:\n","                f1g = f1_score(y_test[mask], pred[mask], average=\"macro\")\n","                print(f\"  {g:>30s} : macroF1={f1g:.3f}  (n={mask.sum()})\")\n"],"metadata":{"id":"ny1O33KEsFaJ","executionInfo":{"status":"ok","timestamp":1765115836885,"user_tz":300,"elapsed":54,"user":{"displayName":"anusha","userId":"11199122514386575205"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"2a0fb3c2-7b47-43d2-eb45-ac0a57184676"},"execution_count":22,"outputs":[{"output_type":"stream","name":"stdout","text":["Fairness: only one Topic class after filtering, skipping.\n"]}]},{"cell_type":"code","source":["# === Robustness slices for Topic (no merging to \"Other\") ===\n","from sklearn.pipeline import Pipeline\n","from sklearn.feature_extraction.text import TfidfVectorizer\n","from sklearn.linear_model import LogisticRegression\n","from sklearn.metrics import f1_score, accuracy_score\n","\n","def add_char_noise(s, p=0.1, rng=None):\n","    rng = rng or np.random.default_rng(SEED)\n","    chars = list(s)\n","    for i in range(len(chars)):\n","        if rng.random() < p:\n","            chars[i] = \"\"  # delete char\n","    return \"\".join(chars)\n","\n","def drop_words(s, frac=0.2):\n","    toks = s.split()\n","    k = int(len(toks) * (1 - frac))\n","    return \" \".join(toks[:max(k, 1)])\n","\n","# 1) Build a Topic dataset WITHOUT merging to \"Other\"\n","use = df.dropna(subset=[\"original_text\", \"Topic\"]).copy()\n","use = use[\n","    (use[\"original_text\"].astype(str).str.strip() != \"\") &\n","    (use[\"Topic\"].astype(str).str.strip() != \"\")\n","]\n","\n","# Drop only truly unusable tiny classes (count < 2), but DO NOT merge\n","counts = use[\"Topic\"].value_counts()\n","keep_labels = counts[counts >= 2].index\n","use = use[use[\"Topic\"].isin(keep_labels)].copy()\n","\n","print(\"Robustness – Topic class counts after filtering:\")\n","print(use[\"Topic\"].value_counts())\n","\n","if use[\"Topic\"].nunique() < 2 or len(use) < 50:\n","    print(\"Robustness: not enough Topic classes or samples; skipping robustness tests.\")\n","else:\n","    # 2) Train/test split (stratified)\n","    from sklearn.model_selection import train_test_split\n","\n","    X_train, X_test, y_train, y_test = train_test_split(\n","        use[\"original_text\"].astype(str),\n","        use[\"Topic\"].astype(str),\n","        test_size=0.2,\n","        random_state=SEED,\n","        stratify=use[\"Topic\"].astype(str)\n","    )\n","\n","    # Safety check in case something still collapses\n","    if len(set(y_train)) < 2:\n","        print(\"Robustness: y_train ended up with one class; skipping.\")\n","    else:\n","        pipe = Pipeline([\n","            (\"tfidf\", TfidfVectorizer(max_features=15000, ngram_range=(1, 2))),\n","            (\"lr\", LogisticRegression(max_iter=400))\n","        ]).fit(X_train, y_train)\n","\n","        # 3) Noise tests\n","        for p in [0.05, 0.10, 0.20]:\n","            noisy = X_test.astype(str).apply(lambda s: add_char_noise(s, p))\n","            yp = pipe.predict(noisy)\n","            print(f\"[char_noise {int(p*100)}%] macroF1={f1_score(y_test, yp, average='macro'):.3f}\")\n","\n","        for frac in [0.1, 0.2, 0.3]:\n","            dr = X_test.astype(str).apply(lambda s: drop_words(s, frac))\n","            yp = pipe.predict(dr)\n","            print(f\"[word_dropout {int(frac*100)}%] macroF1={f1_score(y_test, yp, average='macro'):.3f}\")\n","\n","        # 4) Long-email slice\n","        lengths = X_test.str.len()\n","        thresh = max(1000, int(lengths.quantile(0.9)))\n","        long = X_test[lengths >= thresh]\n","        if len(long) >= 20:\n","            yp = pipe.predict(long)\n","            print(f\"[long emails] n={len(long)}  macroF1={f1_score(y_test.loc[long.index], yp, average='macro'):.3f}\")\n","        else:\n","            print(\"[long emails] not enough long emails for a stable slice.\")\n"],"metadata":{"id":"eL6_FfP1sHGG","executionInfo":{"status":"error","timestamp":1765115962418,"user_tz":300,"elapsed":62,"user":{"displayName":"anusha","userId":"11199122514386575205"}},"colab":{"base_uri":"https://localhost:8080/","height":1000},"outputId":"5ad2ab24-59fc-4f2a-f978-97fb3b71f43b"},"execution_count":25,"outputs":[{"output_type":"stream","name":"stdout","text":["Robustness – Topic class counts after filtering:\n","Topic\n","America's global role and the myth of its decline                                                      3\n","Sudan intelligence memo on Abyei conflict                                                              3\n","Egypt Muslim Brotherhood post-election strategy                                                        3\n","US diplomatic initiative for Georgia-Russia relations                                                  2\n","Ga